#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Bywulf\Jigsawlutioner\Command;
use Symfony\Component\Console\Application;

$application = new Application();

$application->add(new Command\CreateSmallNopModelCommandCommand());
$application->add(new Command\CreateBigNopModelCommandCommand());
$application->add(new Command\CreateCornerDistanceModelCommandCommand());
$application->add(new Command\CreateDepthModelCommandCommand());
$application->add(new Command\AnalyzePiecesCommand());
$application->add(new Command\SolvePuzzleCommand());
$application->add(new Command\FindWeightsForMatcherCommand());
$application->add(new Command\CreateLineDistanceModelCommandCommand());
$application->add(new Command\AnalyzePiecesConsumeCommand());
$application->add(new Command\SaveCornerPointsCommand());
$application->add(new Command\DiffCornerPointsCommand());

$application->run();
